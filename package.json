        "node": ">=18.0.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "globalTeardown": "<rootDir>/jest.teardown.js",
        "testPathIgnorePatterns": [
            "<rootDir>/e2e/",
            "<rootDir>/.next/"
        ],
        "moduleNameMapper": {
